{
	"$schema": "./schema.json",
	"user": {
		"name": null,
		"location": null,
		"timezone": null
	},
	"server": {
		"host": "127.0.0.1",
		"port": 18800,
		"cors": {
			"enabled": true,
			"origins": ["*"]
		}
	},
	"agent": {
		"defaultModel": null,
		"maxIterations": 20,
		"timeout": 300000,
		"systemPrompt": null,
		"debugMode": false,
		"temperature": null,
		"maxTokens": null,
		"toolCallMode": "prompt"
	},
	"network": {
		"timeout": 60000,
		"streamTimeout": 120000,
		"retryCount": 3,
		"retryDelay": 1000
	},
	"providers": {
		"openai": {
			"id": "openai",
			"name": "OpenAI",
			"baseUrl": "${OPENAI_BASE_URL}",
			"apiKey": "${OPENAI_API_KEY}",
			"type": "openai",
			"models": ["gpt-4", "gpt-4-turbo", "gpt-3.5-turbo"],
			"defaultModel": "gpt-4",
			"enabled": true,
			"timeout": 120000,
			"supportsVision": true
		},
		"anthropic": {
			"id": "anthropic",
			"name": "Anthropic",
			"baseUrl": "${ANTHROPIC_BASE_URL}",
			"apiKey": "${ANTHROPIC_API_KEY}",
			"type": "anthropic",
			"models": ["claude-3-5-sonnet-20241022", "claude-3-5-haiku-20241022", "claude-3-opus-20240229"],
			"defaultModel": "claude-3-5-sonnet-20241022",
			"enabled": true,
			"timeout": 120000,
			"supportsVision": true
		}
	},
	"tools": {
		"enabled": ["exec", "browser", "desktop", "screenshot"],
		"exec": {
			"enabled": true,
			"timeout": 30000,
			"sandbox": true
		},
		"browser": {
			"enabled": true,
			"headless": false,
			"timeout": 60000,
			"userDataDir": null,
			"browserPath": null,
			"debugPort": 9223,
			"useIsolatedProfile": false,
			"isolatedProfileDir": null,
			"autoStart": true,
			"smartDetection": true
		},
		"desktop": {
			"enabled": true,
			"mouseSpeed": 1000,
			"keyboardDelay": 50
		},
		"screenshot": {
			"enabled": true,
			"quality": 80,
			"format": "png"
		},
		"web": {
			"enabled": true,
			"timeout": 10000
		}
	},
	"mcp": {
		"enabled": true,
		"servers": []
	},
	"ocr": {
		"enabled": true,
		"baseUrl": "http://localhost:5000",
		"timeout": 30000
	},
	"cron": {
		"enabled": true,
		"jobs": []
	},
	"sandbox": {
		"mode": "standard",
		"custom": {}
	},
	"skills": {
		"enabled": true,
		"directory": "./skills",
		"autoload": true,
		"includeInPrompt": true
	},
	"logging": {
		"level": "info",
		"file": null,
		"console": true
	},
	"data": {
		"directory": "./data",
		"sessions": "./data/sessions",
		"logs": "./data/logs"
	}
}
